generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model questions {
  bad_answer_1   String
  bad_answer_2   String?
  bad_answer_3   String?
  creation_date  DateTime?        @db.Timestamp(6)
  good_answer    String
  id             Int              @id @default(autoincrement())
  question       String
}

model usersresponses {
  creation_date   DateTime? @db.Timestamp(6)
  id              Int       @id @default(autoincrement())
  is_correct      Boolean
  question_id     Int
  time_to_respond Float?
  user_id         String?
  user_response   String
}
